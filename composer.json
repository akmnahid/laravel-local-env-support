{
    "name": "akmnahid/laravel-local-env-support",
    "description": "The laravel-local-env-support plugin allows Laravel developers to seamlessly integrate environment-specific .env.local configurations, enhancing adaptability across different development stages.",
    "authors": [
        {
            "name": "Abu Kahab Mohammad Nahid Hossain",
            "email": "mail@akmnahid.com",
            "role": "Developer",
            "homepage": "https://www.akmnahid.com"
        }
    ],
    "type": "library",
    "keywords": [
        "akmnahid",
        "laravel-env-local-support"
    ],
    "homepage": "https://github.com/akmnahid/laravel-local-env-support",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "LaravelEnvLocal\\": "src/"
        }
    },
    "require": {
        "php": ">=7.3",
        "illuminate/support": ">=8.0",
        "vlucas/phpdotenv": ">=5.5"
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnvLocal\\EnvLocalServiceProvider"
            ]
        }
    }
}
